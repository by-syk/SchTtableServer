<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>SchTtableServer</display-name>
  <welcome-file-list>
    <welcome-file>misc/index.do</welcome-file>
  </welcome-file-list>
  <error-page>
    <error-code>400</error-code>
    <location>/misc/error.do?code=400</location>
  </error-page>
  <error-page>
    <error-code>403</error-code>
    <location>/misc/error.do?code=403</location>
  </error-page>
  <error-page>
    <error-code>404</error-code>
    <location>/misc/error.do?code=404</location>
  </error-page>
  <error-page>
    <error-code>405</error-code>
    <location>/misc/error.do?code=405</location>
  </error-page>
  <error-page>
    <error-code>406</error-code>
    <location>/misc/error.do?code=406</location>
  </error-page>
  <error-page>
    <error-code>500</error-code>
    <location>/misc/error.do?code=500</location>
  </error-page>
  <error-page>
    <error-code>501</error-code>
    <location>/misc/error.do?code=501</location>
  </error-page>
  <error-page>
    <error-code>502</error-code>
    <location>/misc/error.do?code=502</location>
  </error-page>
  <servlet>
    <servlet-name>schttable</servlet-name>
    <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>schttable</servlet-name>
    <url-pattern>*.do</url-pattern>
  </servlet-mapping>
</web-app>